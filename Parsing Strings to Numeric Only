/****** Object:  UserDefinedFunction [dbo].[udf_GetNumeric] ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO

CREATE FUNCTION [dbo].[udf_GetNumeric] (@String [VARCHAR](255)) RETURNS VARCHAR(25)
AS
BEGIN
DECLARE @Numeric INT

SET @Numeric = PATINDEX('%[^0-9]%',@String)

  BEGIN
    WHILE @Numeric > 0
      BEGIN
	    SET @String = STUFF(@String,@Numeric,1,'')
        SET @Numeric = PATINDEX('%[^0-9]%',@String)
      END
  END

RETURN @String

END
GO
